<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.like4u.server.infrastructrue.dao.IGroupsDao">

    <select id="queryGroupsById" resultType="com.like4u.server.infrastructrue.po.Groups">
        SELECT groupId, groupName, groupHead FROM `group` where groupId = #{groupId}
    </select>
</mapper>